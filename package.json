    "dayjs": "^1.11.10",
    "next": "14.1.3",
    "next-auth": "^4.24.7",
    "react": "^18",
    "react-dom": "^18",
    "react-markdown": "^9.0.1",
    "sharp": "^0.33.2",
    "tailwind-datepicker-react": "^1.4.3",
    "uuid": "^9.0.1",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.18",
